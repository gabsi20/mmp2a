<div class="container-fluid">
  <div class="row">
    <div class="col-xs-0 col-sm-3 col-lg-4">
      <h1 class="ios_login">Welcome to Todoify</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-0 col-sm-3 col-lg-4"></div>
    <div class="col-lg-4 col-sm-6 col-xs-12 provider">
      <%= link_to(user_omniauth_authorize_path(:google_oauth2), class: "ios_login_active") do %>
      <p><%= image_tag "google.png", :class => "provider_logo" %>Sign In With Google</p>
      <% end %>
    </div>
    <div class="col-xs-0 col-sm-3 col-lg-4"></div>
  </div>
</div>

